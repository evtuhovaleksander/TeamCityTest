# Clean repository, DerivedData folder and simulator
git reset --hard
git clean -fd
rm -rf ~/Library/Developer/Xcode/DerivedData/TeamCityTest*
xcrun simctl erase all
# Unit tests
xctool \
-scheme TeamCityTest \
-workspace TeamCityTest.xcworkspace \
-destination "platform=iOS Simulator,name=iPhone 6,OS=11.2" \
-sdk iphonesimulator \
-reporter junit:test-results/TEST-UnitTests.xml \
-reporter pretty \
#clean test
