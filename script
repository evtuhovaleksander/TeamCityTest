
git reset --hard
git clean -fd
rm -rf ~/Library/Developer/Xcode/DerivedData/TeamCityTest
xcrun simctl erase all

xctool\
-scheme TEST_SCHEME_NAME\
-workspace TeamCityTest.xcworkspace\
-destination "platform=iOS Simulator,name=iPhone 6,OS=8.3"\
-sdk iphonesimulator\
-reporter junit:test-results/TEST-UnitTests.xml\
-reporter pretty\
clean test
