# Clean repository, DerivedData folder and simulator
git reset --hard
git clean -fd
rm -rf ~/Library/Developer/Xcode/DerivedData/TeamCityTestUITests
xcrun simctl erase all

# Unit tests
xctool \
-scheme TeamCityTestUITests \
-workspace TeamCityTest.xcworkspace \
-destination "platform=iOS Simulator,name=iPhone 6,OS=11.2" \
-sdk iphonesimulator \
build-tests \
run-tests
#clean test
